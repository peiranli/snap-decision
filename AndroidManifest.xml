            android:label="@string/app_name">


        </activity>


        <activity
            android:theme="@android:style/Theme.NoTitleBar"
            android:name="com.example.dots.GraphActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:theme="@android:style/Theme.NoTitleBar"
            android:name="com.example.dots.SplashActivity"></activity>

        <activity
            android:theme="@android:style/Theme.NoTitleBar"
