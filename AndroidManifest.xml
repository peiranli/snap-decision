            android:label="@string/app_name"

            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>
        <activity
            android:name="com.example.dots.LoginSignupActivity"

            android:theme="@android:style/Theme.NoTitleBar">

        </activity>
        <activity
            android:name="com.example.dots.GameActivity"
            android:theme="@android:style/Theme.NoTitleBar"
            >

        </activity>
        <activity
            android:name = "com.example.dots.LimitsGameActivity"
            android:theme="@android:style/Theme.NoTitleBar">

        </activity>
        <activity
            android:theme="@android:style/Theme.NoTitleBar"
            android:name="com.example.dots.NewUserMessageActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name="com.example.dots.TimedGameActivity"
            android:theme="@android:style/Theme.NoTitleBar"></activity>
        <activity
            android:theme="@android:style/Theme.NoTitleBar"
